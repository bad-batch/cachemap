{
  "name": "cachemap",
  "version": "0.2.2",
  "description": "An isomorphic cache that can use Redis, LocalStorage, IndexedDB or an in-memory Map.",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/bad-batch/cachemap",
  "bugs": {
    "url": "https://github.com/bad-batch/cachemap/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bad-batch/cachemap"
  },
  "main": "lib/main/index.js",
  "module": "lib/module/index.js",
  "browser": "lib/browser/index.js",
  "types": "lib/types/index.d.ts",
  "keywords": [
    "cachemap",
    "cache",
    "map",
    "redis",
    "localstorage",
    "indexedDB",
    "isomorphic"
  ],
  "scripts": {
    "analyze:bundle": "webpack-bundle-analyzer \"bundle/stats.json\" \"lib/umd\" --mode static --report \"bundle/report.html\" --no-open",
    "build": "yarn run clean && yarn run test && yarn run compile && yarn run analyze:bundle && yarn run document",
    "clean": "gulp clean",
    "compile": "yarn run compile:main && yarn run compile:types && yarn run compile:module && yarn run compile:browser && yarn run compile:umd",
    "compile:browser": "cross-env BABEL_ENV=browser gulp browser",
    "compile:main": "cross-env BABEL_ENV=main gulp main",
    "compile:module": "cross-env BABEL_ENV=module gulp module",
    "compile:types": "gulp types && yarn run dts-bundle && delete-empty \"lib/types\"",
    "compile:umd": "cross-env BABEL_ENV=browser gulp umd",
    "cut-off": "cut-off",
    "cutoff:post-version": "yarn run document",
    "debug:web": "cross-env BABEL_ENV=debug TEST_ENV=true karma start --no-single-run --reporters progress --browsers",
    "document": "gulp document",
    "dts-bundle": "dts-bundle --name cacheability --main \"lib/types/**/*.d.ts\" --out \"index.d.ts\" --outputAsModuleFolder --removeSource",
    "lint": "yarn run tslint && yarn run markdownLint",
    "markdownLint": "markdownlint README.md",
    "publish-cutoff": "publish-cutoff",
    "test": "yarn run lint && yarn run type-check && yarn run test:web && yarn run test:node",
    "test:node": "cross-env TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT=\"./tsconfig.test.json\" nyc mocha --opts \"./mocha.opts\"",
    "test:web": "cross-env BABEL_ENV=browser TEST_ENV=true karma start --single-run --browsers ChromeHeadless --reporters mocha,coverage-istanbul",
    "tslint": "gulp tslint",
    "type-check": "gulp type-check"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run test"
    }
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0-beta.44",
    "cacheability": "^2.0.1",
    "fakeredis": "^2.0.0",
    "idb": "^2.1.3",
    "iso-logger": "0.0.12",
    "md5": "^2.2.1",
    "object-sizeof": "^1.2.0",
    "promise-worker": "^1.1.1",
    "redis": "^2.8.0"
  },
  "peerDependencies": {
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.44",
    "@babel/plugin-proposal-decorators": "7.0.0-beta.44",
    "@babel/preset-env": "7.0.0-beta.44",
    "@babel/preset-stage-0": "7.0.0-beta.44",
    "@types/chai": "^4.1.4",
    "@types/lodash": "^4.14.110",
    "@types/md5": "^2.1.32",
    "@types/mocha": "^5.2.3",
    "@types/node": "^10.3.6",
    "@types/redis": "^2.8.6",
    "@types/sinon": "^5.0.1",
    "@types/sinon-chai": "^3.2.0",
    "awesome-typescript-loader": "^5.2.0",
    "babel-eslint": "^8.2.5",
    "babel-plugin-lodash": "^3.3.2",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "cross-env": "^5.2.0",
    "cutoff": "^0.0.13",
    "del": "^3.0.0",
    "delete-empty": "^2.0.0",
    "dotenv": "^6.0.0",
    "dts-bundle": "^0.7.3",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typedoc": "^2.2.0",
    "gulp-typescript": "^5.0.0-alpha.2",
    "husky": "1.0.0-rc.8",
    "isomorphic-fetch": "^2.2.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^2.0.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "lodash": "^4.17.5",
    "lodash-webpack-plugin": "^0.11.4",
    "markdownlint-cli": "^0.10.0",
    "merge-stream": "^1.0.1",
    "mocha": "^5.0.5",
    "nyc": "^12.0.2",
    "sinon": "^6.0.1",
    "sinon-chai": "^3.2.0",
    "source-map-loader": "^0.2.3",
    "source-map-support": "^0.5.4",
    "ts-node": "^7.0.0",
    "tslint": "^5.9.1",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "4.12.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-stream": "^4.0.3"
  }
}
