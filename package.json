{
  "name": "cachemap",
  "version": "1.0.0",
  "description": "An isomorphic cache that can use Redis, LocalStorage, IndexedDB or an in-memory Map.",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/bad-batch/cachemap",
  "bugs": {
    "url": "https://github.com/bad-batch/cachemap/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bad-batch/cachemap"
  },
  "keywords": [
    "cachemap",
    "cache",
    "map",
    "redis",
    "localstorage",
    "indexedDB",
    "isomorphic"
  ],
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "yarn run clean:libs && yarn run compile",
    "clean": "concurrently npm:clean:*",
    "clean:node_modules": "lerna exec --parallel -- del-cli ./node_modules",
    "clean:libs": "lerna exec --parallel -- del-cli ./lib",
    "compile": "concurrently npm:compile:*",
    "compile:browser": "cross-env BABEL_ENV=browser lerna exec --parallel -- babel ./src --out-dir ./lib/browser --extensions \".ts\" --config-file ../../babel.config.js",
    "compile:main": "cross-env BABEL_ENV=main lerna exec --parallel -- babel ./src --out-dir ./lib/main --extensions \".ts\" --config-file ../../babel.config.js",
    "compile:module": "cross-env BABEL_ENV=module lerna exec --parallel -- babel ./src --out-dir ./lib/module --extensions \".ts\" --config-file ../../babel.config.js",
    "compile:types": "tsc --build ./packages",
    "cutoff": "lerna-cutoff",
    "init": "yarn && yarn run bootstrap && yarn run compile",
    "lint": "tslint --project ./tsconfig.json",
    "new-package": "new-package",
    "new-package:post": "yarn run bootstrap",
    "publish-cutoff": "publish-lerna-cutoff",
    "test:integration:node": "cross-env BABEL_ENV=test nyc mocha --opts ./mocha.opts",
    "type-check": "tsc --noEmit",
    "wipe": "yarn run wipe:packages && yarn run wipe:repo",
    "wipe:packages": "lerna exec --parallel -- del-cli ./yarn.lock ./node_modules",
    "wipe:repo": "del-cli ./yarn.lock ./node_modules"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run test"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.56",
    "@babel/core": "^7.0.0-beta.56",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.56",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.56",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0-beta.56",
    "@babel/plugin-proposal-function-sent": "^7.0.0-beta.56",
    "@babel/plugin-proposal-json-strings": "^7.0.0-beta.56",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0-beta.56",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0-beta.56",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.56",
    "@babel/plugin-syntax-import-meta": "^7.0.0-beta.56",
    "@babel/preset-env": "^7.0.0-beta.56",
    "@babel/preset-typescript": "^7.0.0-beta.56",
    "@babel/register": "7.0.0-beta.56",
    "@types/chai": "^4.1.4",
    "@types/lodash": "^4.14.110",
    "@types/mocha": "^5.2.4",
    "@types/node": "^10.5.8",
    "@types/sinon": "^5.0.1",
    "@types/sinon-chai": "^3.2.0",
    "awesome-typescript-loader": "^5.2.0",
    "babel-eslint": "^8.2.5",
    "babel-plugin-lodash": "^3.3.2",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "concurrently": "^3.6.1",
    "cross-env": "^5.2.0",
    "cutoff": "^0.0.15",
    "del-cli": "^1.1.0",
    "dotenv": "^6.0.0",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "husky": "1.0.0-rc.8",
    "isomorphic-fetch": "^2.2.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^3.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "lerna": "^3.0.3",
    "lodash": "^4.17.5",
    "lodash-webpack-plugin": "^0.11.4",
    "markdownlint-cli": "^0.12.0",
    "mocha": "^5.0.5",
    "nyc": "^12.0.2",
    "repodog": "^0.0.3",
    "sinon": "^6.0.1",
    "sinon-chai": "^3.2.0",
    "source-map-loader": "^0.2.3",
    "source-map-support": "^0.5.4",
    "ts-node": "^7.0.0",
    "tslint": "^5.9.1",
    "typescript": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "4.16.5",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^3.0.8"
  }
}