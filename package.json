{
  "name": "cachemap",
  "version": "0.1.7",
  "description": "An isomorphic cache that can use Redis, LocalStorage, IndexedDB or an in-memory Map.",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/dylanaubrey/cachemap",
  "bugs": {
    "url": "https://github.com/dylanaubrey/cachemap/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dylanaubrey/cachemap"
  },
  "main": "lib/main/index.js",
  "module": "lib/module/index.js",
  "browser": "lib/browser/index.js",
  "types": "lib/main/index.d.ts",
  "keywords": [
    "cachemap",
    "cache",
    "map",
    "redis",
    "localstorage",
    "indexedDB",
    "isomorphic"
  ],
  "scripts": {
    "analyze:bundle": "webpack-bundle-analyzer bundle/stats.json lib/umd --mode static --report bundle/report.html --no-open",
    "build": "npm run clean && npm run test && npm run compile && npm run analyze:bundle && npm run document",
    "clean": "gulp clean",
    "compile": "npm run compile:main && npm run compile:module && npm run compile:browser && npm run compile:umd",
    "compile:browser": "cross-env BABEL_ENV=browser gulp browser",
    "compile:main": "cross-env BABEL_ENV=main gulp main",
    "compile:module": "cross-env BABEL_ENV=module gulp module",
    "compile:umd": "cross-env BABEL_ENV=browser gulp umd",
    "debug:web": "cross-env BABEL_ENV=browser TEST_ENV=true karma start --no-single-run --reporters progress --browsers",
    "document": "gulp document",
    "lint": "gulp lint",
    "publish:pkg": "npm run build && npm publish",
    "test": "npm run lint && npm run type-check && npm run test:web && npm run test:node",
    "test:node": "cross-env TS_NODE_PROJECT=\"./tsconfig.test.json\" nyc mocha --opts \"./mocha.opts\"",
    "test:labs": "cross-env BABEL_ENV=browser TEST_ENV=true karma start karma.conf.labs.js",
    "test:web": "cross-env BABEL_ENV=browser TEST_ENV=true karma start --single-run --browsers ChromeHeadless --reporters mocha,coverage-istanbul",
    "type-check": "gulp type-check"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.44",
    "cacheability": "^0.1.0",
    "fakeredis": "^2.0.0",
    "idb": "^2.1.1",
    "iso-logger": "0.0.12",
    "md5": "^2.2.1",
    "object-sizeof": "^1.2.0",
    "promise-worker": "^1.1.1",
    "redis": "^2.8.0"
  },
  "peerDependencies": {
    "lodash": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-stage-0": "^7.0.0-beta.44",
    "@types/chai": "^4.1.2",
    "@types/lodash": "^4.14.106",
    "@types/md5": "^2.1.32",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.2",
    "@types/redis": "^2.8.6",
    "@types/sinon": "^4.3.1",
    "@types/sinon-chai": "^2.7.29",
    "awesome-typescript-loader": "^4.0.1",
    "babel-eslint": "^8.2.2",
    "babel-plugin-lodash": "^3.3.2",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.4",
    "del": "^3.0.0",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typedoc": "^2.2.0",
    "gulp-typescript": "^4.0.2",
    "isomorphic-fetch": "^2.2.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.2.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "lodash": "^4.17.5",
    "lodash-webpack-plugin": "^0.11.4",
    "merge-stream": "^1.0.1",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0",
    "source-map-loader": "^0.2.3",
    "source-map-support": "^0.5.4",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "3.10.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-stream": "^4.0.3"
  }
}
