{
  "name": "cachemap",
  "version": "1.1.0",
  "description": "An isomorphic cache that can use Redis, LocalStorage, IndexedDB or an in-memory Map.",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/badbatch/cachemap",
  "bugs": {
    "url": "https://github.com/badbatch/cachemap/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/badbatch/cachemap"
  },
  "keywords": [
    "cachemap",
    "cache",
    "map",
    "redis",
    "localstorage",
    "indexedDB",
    "isomorphic"
  ],
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "yarn run clean:libs && yarn run compile",
    "clean": "yarn run clean:packages && yarn run clean:libs && yarn run clean:docs && yarn run clean:repo",
    "clean:docs": "lerna exec --parallel -- del-cli ./docs",
    "clean:libs": "lerna exec --parallel -- del-cli ./lib",
    "clean:packages": "lerna exec --parallel -- del-cli ./node_modules",
    "compile": "lerna run compile --parallel",
    "compile:browser": "lerna run compile:browser --parallel",
    "compile:main": "lerna run compile:main --parallel",
    "compile:module": "lerna run compile:module --parallel",
    "compile:types": "lerna run compile:types --parallel",
    "cutoff": "cutoff-lerna",
    "cutoff:post-version": "yarn run compile && yarn run docs",
    "debug:integration:web": "cross-env BABEL_ENV=debug TEST_ENV=web karma start --no-single-run --reporters progress --log-level debug --browsers",
    "debug:integration:worker": "cross-env BABEL_ENV=debug TEST_ENV=worker karma start --no-single-run --reporters progress --log-level debug --browsers",
    "docs": "lerna exec --parallel -- typedoc --includes ./src/main --out ./docs --options ../../typedoc.js",
    "init": "yarn && yarn run bootstrap && yarn run compile",
    "lint": "tslint --project ./tsconfig.json",
    "new-package": "new-package",
    "new-package:post": "yarn run bootstrap",
    "publish-cutoff": "publish-lerna-cutoff",
    "test:integration": "concurrently npm:test:integration:*",
    "test:integration:node": "cross-env BABEL_ENV=test mocha --opts ./mocha.opts",
    "test:integration:web": "cross-env BABEL_ENV=web TEST_ENV=web karma start --single-run --browsers ChromeHeadless --reporters mocha",
    "test:integration:worker": "cross-env BABEL_ENV=web TEST_ENV=worker karma start --single-run --browsers ChromeHeadless --reporters mocha",
    "type-check": "tsc --noEmit",
    "wipe": "yarn run wipe:packages && yarn run wipe:repo",
    "wipe:packages": "lerna exec --parallel -- del-cli ./yarn.lock ./node_modules",
    "wipe:repo": "del-cli ./yarn.lock ./node_modules"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint && yarn run type-check && yarn run test:integration"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "7.4.4",
    "@babel/runtime": "^7.4.4",
    "@babel/runtime-corejs3": "^7.4.4",
    "@types/chai": "^4.1.7",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.8",
    "@types/sinon": "^7.0.11",
    "@types/sinon-chai": "^3.2.2",
    "@types/uuid": "^3.4.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-lodash": "^3.3.2",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "core-js": "^3.0.1",
    "cross-env": "^5.2.0",
    "cutoff": "^0.2.1",
    "del-cli": "^1.1.0",
    "dotenv": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "fs-extra": "^7.0.1",
    "husky": "2.1.0",
    "isomorphic-fetch": "^2.2.1",
    "karma": "^4.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "3.0.5",
    "lerna": "^3.13.4",
    "lodash": "^4.17.11",
    "lodash-webpack-plugin": "^0.11.4",
    "markdownlint-cli": "^0.15.0",
    "mocha": "^6.1.4",
    "repodog": "^0.0.3",
    "rollup": "^1.10.1",
    "rollup-plugin-analyzer": "^3.0.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-terser": "^4.0.4",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "source-map-loader": "^0.2.4",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^1.2.0",
    "typescript": "^3.4.5",
    "webpack": "4.30.0"
  }
}
