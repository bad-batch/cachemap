{
  "name": "@cachemap/local-storage",
  "version": "1.0.4",
  "description": "The Cachemap LocalStorage module.",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/badbatch/cachemap",
  "bugs": {
    "url": "https://github.com/badbatch/cachemap/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/badbatch/cachemap"
  },
  "main": "lib/main/index.js",
  "module": "lib/module/index.js",
  "browser": "lib/browser/index.js",
  "types": "lib/types/index.d.ts",
  "keywords": [
    "cachemap",
    "cache",
    "LocalStorage"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "compile": "concurrently npm:compile:*",
    "compile:browser": "cross-env BABEL_ENV=web rollup -c ../../rollup.config.js",
    "compile:main": "cross-env BABEL_ENV=main babel ./src --out-dir ./lib/main --extensions \".ts\" --config-file ../../babel.config.js",
    "compile:module": "cross-env BABEL_ENV=module babel ./src --out-dir ./lib/module --extensions \".ts\" --config-file ../../babel.config.js",
    "compile:types": "tsc --build"
  },
  "peerDependencies": {
    "@babel/runtime-corejs3": "^7.0.0",
    "core-js": "^3.0",
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "@cachemap/core": "1.0.4"
  }
}